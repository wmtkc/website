{
  "connection": "default",
  "collectionName": "blogposts",
  "info": {
    "name": "blogpost",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "url": {
      "unique": true,
      "required": true,
      "type": "string"
    },
    "title": {
      "unique": true,
      "required": true,
      "type": "string"
    },
    "body": {
      "type": "text"
    },
    "cover": {
      "model": "file",
      "via": "related",
      "plugin": "upload"
    },
    "tags": {
      "collection": "tag",
      "via": "blogposts",
      "dominant": true
    },
    "published": {
      "type": "boolean"
    }
  }
}